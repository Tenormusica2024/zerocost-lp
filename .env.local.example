# Supabase（サーバーサイド専用 / service_role で RLS バイパス）
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJ...  # Supabase > Settings > API > service_role key

# Supabase Auth（SSR / クライアントサイド用）
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ...  # Supabase > Settings > API > anon key

# App URL
NEXT_PUBLIC_APP_URL=https://your-domain.com

# zerocost-router Worker URL
NEXT_PUBLIC_ROUTER_BASE=https://zerocost-router.dragonrondo.workers.dev

# Stripe（テスト: sk_test_... / 本番: sk_live_...）
STRIPE_SECRET_KEY=sk_test_...   # Stripe Dashboard > Developers > API keys > Secret key
STRIPE_WEBHOOK_SECRET=whsec_... # Stripe Dashboard > Developers > Webhooks > Signing secret
